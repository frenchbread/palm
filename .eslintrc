{
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "es6": true
  },
  "extends": [
    "airbnb-base"
  ],
  "rules": {
    "arrow-parens": [2,"as-needed"],
    "comma-dangle": [2,"never"],
    "space-before-function-paren": [
      2,
      {
        "anonymous": "always",
        "named": "always"
      }
    ],
    "no-underscore-dangle": 0,
    "semi": [2, "never"]
  }
}
