{
  "name": "palm",
  "version": "0.0.6",
  "description": "Just a palm.",
  "main": "index.js",
  "scripts": {
    "start": "nodemon --exec babel-node --presets es2015 ./src/server.js",
    "build": "node_modules/babel-cli/bin/babel.js src --out-dir lib",
    "test": "npm build && ava",
    "prepublish": "npm build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/frenchbread/palm.git"
  },
  "keywords": [
    "palm"
  ],
  "author": "Damir Mustafin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/frenchbread/palm/issues"
  },
  "homepage": "https://github.com/frenchbread/palm#readme",
  "dependencies": {
    "got": "^6.3.0",
    "lodash": "^4.14.2"
  },
  "devDependencies": {
    "ava": "^0.15.2",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.11.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-eslint": "^6.1.2",
    "eslint": "^3.4.0",
    "eslint-config-airbnb-base": "^5.0.3",
    "eslint-import-resolver-meteor": "^0.3.3",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-meteor": "^4.0.0"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "parserOptions": {
      "allowImportExportEverywhere": true
    },
    "plugins": [
      "meteor"
    ],
    "extends": [
      "airbnb-base",
      "plugin:meteor/recommended"
    ],
    "settings": {
      "import/resolver": "meteor"
    },
    "rules": {
      "comma-dangle": [
        2,
        "always-multiline"
      ],
      "import/no-extraneous-dependencies": 0,
      "space-before-function-paren": [
        2,
        {
          "anonymous": "always",
          "named": "always"
        }
      ],
      "no-underscore-dangle": [
        2,
        {
          "allow": [
            "_id",
            "__",
            "_host",
            "_token",
            "_parent",
            "_recipient",
            "_offset",
            "_updateOffset",
            "_getUpdates",
            "_initModule"
          ]
        }
      ]
    }
  }
}
